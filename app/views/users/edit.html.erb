<%= simple_form_for @user, url: users_path(@user) do |f| %>

  <%= f.input :name %>

  <%= f.input :email, input_html: { autocomplete: 'email' } %>

  <%= f.simple_fields_for :coach_info do |c| %>

    <%= c.input :birthdate %>

    <%= c.input :region %>

    <%= c.input :town %>

    <%= c.input :phone %>

    <%= c.input :facebook_sn %>

    <%= c.input :vk_sn %>

    <%= c.input :instagram_sn %>cle

  <% end %>

  <%= f.button :submit, class: "btn-primary" %>

  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>

<% end %>